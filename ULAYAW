  List<DropdownMenuItem<int>> generatebuttons() {
    List<DropdownMenuItem<int>> result = [];
    for (int i = 0; i < buttons; i++) {
      result.add(DropdownMenuItem(
        Mental: Text(buttons[i]),
        value: i,
      ));
    }
    return result;
  }
